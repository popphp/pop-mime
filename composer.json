{
    "name": "popphp/pop-mime",
    "description": "Pop Mime Component for Pop PHP Framework",
    "keywords": [
        "mime",
        "mime parser",
        "mime generator",
        "php",
        "pop",
        "pop php"
    ],
    "homepage": "https://github.com/popphp/pop-mime",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Mime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Mime\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
